<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="AspNetMvc4Vs15" namespace="AspNetMvc4Vs15">
  <class name="MyFile" dynamic-update="true">
    <cache usage="read-write"/>
    <id name="Id">
      <generator class="native"/>
    </id>
    <property name="MyDocFile" type="BinaryBlob"/>
    <property name="MyDocName"/>
    <one-to-one name="Document" class="Document"/>
  </class>
</hibernate-mapping>