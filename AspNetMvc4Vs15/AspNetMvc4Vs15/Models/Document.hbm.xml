<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="AspNetMvc4Vs15" namespace="AspNetMvc4Vs15">
  <class name="Document" dynamic-update="true">
    <cache usage="read-write"/>
    <id name="Id" type="int">
      <generator class="native"/>
    </id>
    <property name="Name" not-null="true"/>
    <property name="Date" not-null="true"/>
    <one-to-one name="MyDoc"  class="MyFile" lazy="false" constrained="true" cascade="all"/>
    <many-to-one  name="MyUser" class="Person" column="Person_Id" cascade="save-update"/>
  </class>
</hibernate-mapping>
